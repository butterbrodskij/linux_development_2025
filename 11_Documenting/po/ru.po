#: src/guesser.c:98
#, c-format
msgid "Usage: %s [OPTION]\n"
msgstr "Использование: %s [ПАРАМЕТР]\n"

#: src/guesser.c:99
#, c-format
msgid ""
"Guess the number game using binary search algorithm.\n"
"\n"
msgstr ""
"Игра 'Угадай число' с использованием алгоритма бинарного поиска.\n"
"\n"

#: src/guesser.c:100
#, c-format
msgid "Options:\n"
msgstr "Параметры:\n"

#: src/guesser.c:101
#, c-format
msgid "  -h, --help     display this help and exit\n"
msgstr "  -h, --help     показать эту справку и выйти\n"

#: src/guesser.c:102
#, c-format
msgid ""
"  -r, --roman    use Roman numerals instead of Arabic\n"
"\n"
msgstr ""
"  -r, --roman    использовать римские цифры вместо арабских\n"
"\n"

#: src/guesser.c:103
#, c-format
msgid "Description:\n"
msgstr "Описание\n"

#: src/guesser.c:104
#, c-format
msgid "  The program will try to guess your number between %d and %d\n"
msgstr "  Программа будет пытаться угадать ваше число от %d до %d,\n"

#: src/guesser.c:105
#, c-format
msgid ""
"  by asking questions. Answer with 'yes'/'no' or 'y'/'n' (also supports "
"Russian).\n"
"\n"
msgstr ""
"  задавая вопросы. Отвечайте 'да'/'нет' или 'д'/'н' (также поддерживается "
"английский язык).\n"
"\n"

#: src/guesser.c:134
#, c-format
msgid "Error: Unknown option '%s'\n"
msgstr "Ошибка: Неизвестный параметр '%s'\n"

#: src/guesser.c:135
#, c-format
msgid "Try '%s --help' for more information.\n"
msgstr "Попробуйте '%s --help' для более детальной информации.\n"

#: src/guesser.c:141
#, c-format
msgid "guess a number from %s to %s\n"
msgstr "загадайте число от %s до %s\n"

#: src/guesser.c:147
#, c-format
msgid ""
"is your number greater than %s? (Yes/No)\n"
"> "
msgstr ""
"ваше число больше %s? (Да/Нет)\n"
"> "

#: src/guesser.c:149
#, c-format
msgid "unexpected EOF\n"
msgstr "некорректный конец ввода\n"

#: src/guesser.c:165
#, c-format
msgid "unexpected answer: \"%s\"\n"
msgstr "некорректный ответ: \"%s\"\n"

#: src/guesser.c:169
#, c-format
msgid "your number: %s\n"
msgstr "ваше число: %s\n"
